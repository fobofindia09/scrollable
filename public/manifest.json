{
  "name": "Scrollable",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": ["storage"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/static/css/content.css"],
      "js": [
        "/static/js/0.chunk.js",
        "/static/js/3.chunk.js",
        "/static/js/content.js"
      ]
    }
  ],
  "icons": {
    "16": "Scrollable16.png",
    "48": "Scrollable48.png",
    "128": "Scrollable128.png"
  }
}
